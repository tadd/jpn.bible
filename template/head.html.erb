<script>
 function updateColorScheme(ev) {
     const e = document.documentElement;
     e.dataset.bsTheme = ev.matches ? 'dark' : '';
 }
 const q = window.matchMedia('(prefers-color-scheme: dark)');
 q.addEventListener('change', ev => updateColorScheme(ev));
 updateColorScheme(q); // Call here to avoid flicker as much as possible
</script>
